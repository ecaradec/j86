FUNCTION sum(a, b) {
    RETURN a + b;
}

FUNCTION main() {
    a = 1;

    IF( a == 1 ) {
        print("IF OK");
    } ELSE {
        print("IF NOK");
    }

    a = sum(1,1);
    IF( a == 2) {
        print("CALL OK");
    } ELSE {
        print("CALL NOK");
    }

    a = 5;
    WHILE( a != 0 ) {
        a = a - 1;
    }
    IF( a == 0 ) {
        print("WHILE OK");
    } ELSE {
        print("WHILE NOK");
    }
}